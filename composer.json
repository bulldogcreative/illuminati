{
    "name": "bulldog/illuminati",
    "description": "Bulldog Laravel stuff so far",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Durfee",
            "email": "levi.durfee@gmail.com",
            "homepage": "https://www.bulldogcreative.com"
        }
    ],
    "homepage": "https://github.com/bulldogcreative/illuminati",
    "keywords": ["Laravel", "Illuminati"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bulldog\\Illuminati\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bulldog\\Illuminati\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bulldog\\Illuminati\\IlluminatiServiceProvider"
            ],
            "aliases": {
                "Illuminati": "Bulldog\\Illuminati\\Facades\\Illuminati"
            }
        }
    }
}
